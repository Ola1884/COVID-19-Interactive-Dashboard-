{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "assets/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(?<asset>.*)",
      "dest": "/assets/$asset"
    },
    {
      "src": "(.*)",
      "dest": "app.py"
    }
  ]
}
